<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="VolunteerNotesApp.Views.SubmitView"
             xmlns:vm="using:VolunteerNotesApp.ViewModels.SubmitViewModel"
             HorizontalAlignment="Center">
  <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
    <TextBlock Text="SubmitView: displays a form to submit new notes." HorizontalAlignment="Center"/>
    <Button HorizontalAlignment="Center" Margin="0,20,0,0" x:CompileBindings="False" Command="{Binding $parent[Window].DataContext.Home}">Home</Button>
  </StackPanel>
</UserControl>
