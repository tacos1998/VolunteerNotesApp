<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="VolunteerNotesApp.Views.HomeView">
  <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
      <TextBlock Text="Welcome to VolunteerNotes!" HorizontalAlignment="Center"/>
      <Button HorizontalAlignment="Center" Margin="0,20,0,0" x:CompileBindings="False" Command="{Binding $parent[Window].DataContext.ViewNotes}">View Notes</Button>
      <Button HorizontalAlignment="Center" Margin="0,20,0,0">Submit Notes</Button>
  </StackPanel>
</UserControl>
